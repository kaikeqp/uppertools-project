@model IEnumerable<UpperToolsProject.Models.Qsa>

@{
    ViewData["Title"] = "Details - Socios";
}

<h1>Informações</h1>
<div>
    <table class="col-sm table-bordered table-striped ">
        <thead>
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.EmpresaCnpj)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Nome)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Qual)
                </th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {

                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.EmpresaCnpj)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Nome)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Qual)
                    </td>
                </tr>

            }
        </tbody>
    </table>
</div>
<form asp-action="Details" asp-controller="Empresas">
    <input type="hidden" name="Cnpj" value="@(Model.First().EmpresaCnpj)" />
    <input class="btn btn-outline-info" type="submit" value="Voltar" />
</form>


